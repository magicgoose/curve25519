ifndef config
  config=release
endif
export config

PROJECTS := curve25519-cli

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

curve25519-cli: 
	@echo "==== Building curve25519-cli ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f curve25519-cli.make

clean:
	@${MAKE} --no-print-directory -C . -f curve25519-cli.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   curve25519-cli"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
